<%- include('partials/header') %>
<h1>Register</h1>
<form action="/course_dept" method="POST">
<label>Choose Department</label>
<select name="department">
    <option value="Computer Engineering">Computer Engineering</option>
</select>
<label>Choose Semester</label>
<select name="semester">
    <option value="1">1</option>
    <option value="2">2</option>
    <option value="3">3</option>
    <option value="4">4</option>
    <option value="5">5</option>
    <option value="6">6</option>
    <option value="7">7</option>
    <option value="8">8</option>
</select>
<label>Choose Year</label>
<select name="year">
    <option value="1">1</option>
    <option value="2">2</option>
    <option value="3">3</option>
    <option value="4">4</option>
</select>
<label>Enter CGPA</label>
<input type = "number" class="form-control" name = "cgpa" placeholder = "CGPA" min="0" max="10" step="0.1" required>
<label>Result_ID</label>
<input type = "number" class="form-control" name = "result_id" placeholder = "Result_ID" required>
<label>Choose Course</label>
<select name="course">
    <option value="CS-101">CS-101</option>
    <option value="CS-102">CS-102</option>
</select>
<label>Enter Grade Obtained for Course</label>
<select name="grade">
    <option value="A">A</option>
    <option value="B">B</option>
    <option value="C">C</option>
    <option value="D">D</option>
    <option value="E">E</option>
    <option value="F">F</option>
</select>
<button type="submit">Submit</button>

<%- include('partials/footer') %>
